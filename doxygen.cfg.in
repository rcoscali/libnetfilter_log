# Difference with default Doxyfile 1.8.20
PROJECT_NAME           = @PACKAGE@
PROJECT_NUMBER         = @VERSION@
OUTPUT_DIRECTORY       = doxygen
ABBREVIATE_BRIEF       =
FULL_PATH_NAMES        = NO
TAB_SIZE               = 8
OPTIMIZE_OUTPUT_FOR_C  = YES
INPUT                  = .
FILE_PATTERNS          = *.c
RECURSIVE              = YES
EXCLUDE_SYMBOLS        = nflog_g_handle \
			 nflog_handle \
			 ipulog_errmap_t \
			 ipulog_handle
EXAMPLE_PATTERNS       =
