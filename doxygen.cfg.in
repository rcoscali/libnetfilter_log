ALPHABETICAL_INDEX     = NO
GENERATE_LATEX         = NO
LATEX_CMD_NAME         = latex
GENERATE_MAN           = YES
HAVE_DOT               = YES
DOT_TRANSPARENT        = YES
SEARCHENGINE           = NO
